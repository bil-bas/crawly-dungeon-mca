{
    "compilerOptions": {
        "target": "ES5",
        "noImplicitAny": true,
        "outDir": "built",
        "rootDir": ".",
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitReturns": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "built/**",
        "pxt_modules/**/*test.ts"
    ]
}